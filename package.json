{
  "name": "ghostdeps",
  "version": "0.2.0",
  "description": "Scan and visualize npm package dependency health, security vulnerabilities, and maintenance status before installation",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Quinn Singletary"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qsingletary/ghostdeps.git"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf .next node_modules"
  },
  "dependencies": {
    "@changesets/cli": "^2.29.8",
    "@upstash/redis": "^1.36.1",
    "next": "16.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "jsdom": "^27.4.0",
    "prettier": "^3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5",
    "vitest": "^4.0.17"
  }
}
